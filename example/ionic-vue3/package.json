{
  "name": "ionic-vue3",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "setup:local-env": "node ../../scripts/setup-local-env.js",
    "predev": "yarn setup:local-env",
    "prebuild": "yarn setup:local-env",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org sentry-sdks --project capacitor ./dist && sentry-cli sourcemaps upload --org sentry-sdks --project capacitor ./dist",
    "preview": "vite preview",
    "sync": "ionic cap copy & ionic cap sync",
    "ios": "ionic cap open ios",
    "android": "ionic cap open android",
    "bump:sample-pod": "cd ios/App && pod update Sentry/HybridSDK && pod install || node ../../../../scripts/sample-skip-pod-windows.js",
    "lint": "eslint ."
  },
  "dependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/app": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/ios": "^8.0.0",
    "@capacitor/keyboard": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@ionic/vue": "^8.0.0",
    "@ionic/vue-router": "^8.0.0",
    "@sentry/capacitor": "file:.yalc/@sentry/capacitor",
    "@sentry/cli": "^2.58.4",
    "@sentry/vite-plugin": "^2.10.3",
    "@sentry/vue": "10.36.0",
    "ionicons": "^8.0.0",
    "vue": "^3.5.0",
    "vue-facing-decorator": "^4.0.0",
    "vue-router": "^5.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^8.0.0",
    "@vitejs/plugin-legacy": "^7.0.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-typescript": "^14.0.0",
    "@vue/test-utils": "^2.4.0",
    "cypress": "^15.0.0",
    "eslint": "^8.35.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^28.0.0",
    "terser": "^5.4.0",
    "typescript": "^5.1.6",
    "vite": "^5.4.21",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.0.24"
  },
  "resolutions": {
    "form-data": "4.0.4"
  },
  "description": "Sentry Capacitor Vue3 sample"
}
